body,*,* *,* * *,* * * *{font-family:"Trebuchet MS",Verdana,Arial,Helvetica,sans-serif;font-size:13px;}
body{margin:0;padding:0;background-color:#56130a;}
p{line-height:20px;}
A{color:#009;}
A.visited{color:#669;}
A:hover{color:#2275ff;}
sup{font-size:11px;vertical-align:baseline;position:relative;top:-5px;}
sub{font-size:11px;vertical-align:baseline;position:relative;top:1px;}
td#content{background-image:url(/images/interface/globebt.jpg);background-repeat:no-repeat;width:769px;background-color:#ffeed8;}
div#sbody{padding:0 20px 15px 20px;}
td#lbg,td#rbg{background-image:url(/images/interface/vertbg.jpg);background-repeat:repeat-x;}
td#mmenu{width:461px;}
div.breadcrumb{margin:4px 10px 0 10px;}
div.breadcrumb,div.breadcrumb strong{font-size:11px;color:#6a1a0a;font-weight:bold;}
a.breadcrumb{color:#009;font-size:11px;font-weight:normal;}
a.breadcrumbmenu:hover{color:#6a1a0a;font-weight:normal;}
div#ssearch{text-align:right;padding:3px;white-space:nowrap;padding-bottom:15px;}
div#ssearch form{padding:0;margin:0;}
div#ssearch form input.qry{width:200px;font-size:12px;}
div#sfoot{background-color:#e87a33;padding:10px;color:white;text-align:center;}
div#sfoot1 a,div#sfoot1 a:active,div#sfoot1 a:visited,div#sfoot1 a:hover{font-weight:bold;text-decoration:none;line-height:15px;color:white;font-size:13px;}
div#sfoot2 a,div#sfoot a:active,div#sfoot2 a:visited,div#sfoot2 a:hover{text-decoration:none;line-height:15px;color:white;font-size:13px;}
div.boxttl{font-family:Georgia,"Times New Roman",Times,serif;font-size:13px;color:#3f0101;font-weight:bold;}
div.newsitem{padding-top:7px;}
div.newsdt{color:#d5771c;font-weight:bold;font-size:11px;white-space:nowrap;padding-right:3px;}
a.newsitem,a.newsitem:visited,a.newsitem:hover,a.newsitem:active{color:#009;text-decoration:none;font-size:11px;}
td.subscribe{color:#9d451e;font-size:11px;}
input.subscribeemail{width:110px;color:black;font-size:11px;}
div.gosubscribe{text-align:center;}
h1,h2,h3,h4,h5,h6{color:#56130a;border-bottom:1px solid #d7791c;font-weight:bold;}
h1{padding-top:0;margin-top:0;font-weight:bold;font-size:26px;border-bottom:3px solid #d7791c;}
h2{font-size:22px;}
h3{padding-top:10px;font-size:19px;}
h4{font-size:15px;}
h5{font-style:italic;text-indent:1em;font-size:14px;}
div.boxmenuttl{font-weight:bold;color:white;font-size:13px;margin-bottom:10px;}
div.boxmenubody{font-size:11px;font-weight:normal;color:white;}
td.boxmenubody{background-color:#8c1a02;background-image:url(/images/interface/boxmenu/bodybg.jpg);background-repeat:repeat-x;}
a.sidemenu{font-size:11px;font-weight:normal;color:white;text-decoration:none;}
td.sidemenu{padding-bottom:7px;}
td.smsubheading{font-size:12px;font-weight:bold;color:white;text-decoration:none;}
input.button{background-color:#56130a;color:white;font-weight:bold;}
fieldset,legend{border-width:0;}
p.pithy{font-size:13px;font-weight:bold;color:#6a1a0a;}
B{font-weight:bold;}
a.bodymenu{color:#009;font-size:13px;font-weight:bold;}
a.bodymenu:hover{color:#996800;}
a.topmenu{color:#009;font-size:12px;font-weight:bold;}
a.topmenu:hover{color:#996800;}
.topmenu{color:#d3d3a9;font-size:12px;font-weight:bold;}
a.topmenu2{color:#009;font-size:12px;}
a.topmenu:hover{color:#996800;}
.topmenu2{color:#cc9;font-size:12px;}
a.secondmenu{color:#009;font-size:12px;font-weight:bold;}
a.secondmenu:hover{color:#996800;}
.secondmenubox{background-color:#f9e1ca;border:2px solid #56130a;padding:5px;text-align:left;width:330px;font-size:12px;}
.formlbl{color:#6a1a0a;font-size:13px;font-weight:bold;text-align:right;}
.formlblleft{color:#6a1a0a;font-size:13px;font-weight:bold;text-align:left;}
.forminst{color:#6a1a0a;font-size:12px;}
.sendtofriend_link{font-size:12px;color:#009;}
.title{background-color:#009;padding:5px;border:0;font-size:13px;font-weight:bold;color:white;}
.info{padding-top:6px;font-size:small;line-height:16px;}
.label{padding-right:20px;font-weight:bold;}
.instruct{font-size:xx-small;}
.input{font-weight:bold;}
.submit{font-weight:bold;}
.caption{font:bold 12px verdana,arial,sans-serif;color:#696560;}
.captionleft{font-family:Verdana,Arial,Helvetica,sans-serif;font-size:13px;color:#009;font-style:italic;}
.captionzoom{font-weight:bold;font-size:23px;}
td.sidemenuhead{font-weight:bold;color:white;background-color:#2e6296;font-size:11px;padding:3px 0 3px 0;}
a.relatedlink{font-size:14px;font-weight:bold;}
div.privacy{text-align:right;padding-bottom:2px;}
a.privacy{font-size:10px;color:#3e82c6;}
a.privacy:hover{color:#1e4164;}
div.PagingNav{text-align:right;font-size:11px;margin-bottom:5px;}
div.PagingNav a{font-weight:bold;text-decoration:none;}
.chapt{font-family:arial,verdana,sans-serif;color:black;font-style:normal;font-weight:bold;font-size:14px;}
.highlightbody{padding:5px;}
.highlightbody,.highlightbody *{font-size:11px;}
div.mtgLocation{font-size:15px;font-weight:bold;display:inline;}
div.mtginfo{margin:5px 0px 20px 20px;}
div.panmapnote{text-align:center;border:1px solid black;border-bottom:none;width:523px;}
.nobr{white-space:nowrap;display:inline;}
.addthis_toolbox{padding-top:25px;padding-top:5px;border-top:1px solid #cccccc;}